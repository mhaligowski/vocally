{
  "name": "vega-time",
  "version": "1.1.0",
  "description": "JavaScript date/time utilities for Vega.",
  "keywords": [
    "vega",
    "date",
    "time",
    "utilities"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "main": "build/vega-time.js",
  "module": "index",
  "types": "index.d.ts",
  "repository": "vega/vega",
  "scripts": {
    "rollup": "rollup -g d3-array:d3,d3-time:d3,d3-time-format:d3,vega-util:vega -f umd -n vega -o build/vega-time.js -- index.js",
    "prebuild": "rimraf build && mkdir build",
    "build": "yarn rollup",
    "postbuild": "terser build/vega-time.js -c -m -o build/vega-time.min.js",
    "pretest": "yarn prebuild && yarn rollup",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "d3-array": "^2.4.0",
    "d3-time": "^1.1.0",
    "d3-time-format": "^2.2.3",
    "vega-util": "^1.13.2"
  },
  "gitHead": "35e31c5c6b54db9dc3a577b5adad8d15ec274d32"

,"_resolved": "https://registry.npmjs.org/vega-time/-/vega-time-1.1.0.tgz"
